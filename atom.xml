---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Brian Scaturro</title>
    <link href="http://intentionjs.com/atom.xml" rel="self"/>
    <link href="http://intentionjs.com/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://intentionjs.com/</id>
    <author>
        <name>Intentionjs</name>
        <email>call@1800joe.com</email>
    </author>
    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://intentionjs.com{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://intentionjs.com{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}
</feed>